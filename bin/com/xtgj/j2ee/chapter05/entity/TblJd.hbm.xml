<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>    
	<class name="com.xtgj.j2ee.chapter05.entity.TblJd" table="TBL_JD" schema="dbo" catalog="zf" >
      <id name="jdid" column="jdid" type="java.lang.Integer">
        	<generator class="identity"></generator>
        </id>
        <property name="jd" column="jd" type="java.lang.String" not-null="false"/>
        <many-to-one name="qx" class="com.xtgj.j2ee.chapter05.entity.TblQx" column="qxid" lazy="false"></many-to-one> 
    </class>   
</hibernate-mapping>
